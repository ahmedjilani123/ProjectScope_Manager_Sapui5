<mvc:View xmlns:tnt="sap.tnt"
    xmlns:form="sap.ui.layout.form"
    controllerName="cal.as.sap.calendarcustom.controller.Range1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:obj="sap.uxap"
    xmlns:layout="sap.ui.layout"
    xmlns="sap.m"
     xmlns:core="sap.ui.core" 
    xmlns:cards="sap.f.cards"
    xmlns:f="sap.f"
>

			
           
           
             <f:FlexibleColumnLayout  layout="{ColumnLayout>/FLayout}" id="FlexLayout" >

                <f:beginColumnPages>
                 <core:Fragment  fragmentName="cal.as.sap.calendarcustom.view.Range1" type="XML"/>
                           <!-- <mvc:XMLView viewName="cal.as.sap.calendarcustom.view.Range1"/> -->
                </f:beginColumnPages>
                <f:midColumnPages>
                    <Page  showHeader="true" title="Details" showNavButton="true" navButtonPress="handleCloseSideScreenPress" >
                    <headerContent>
                    <ToggleButton id="toggleId" pressed="true" icon="sap-icon://less" press="ShowFromInputPress"></ToggleButton>
                    <Button ></Button>
                    </headerContent>
                   
                <content>
                  <core:Fragment  fragmentName="cal.as.sap.calendarcustom.view.DetailPage" type="XML"/>
               
                    <!-- <mvc:XMLView viewName="cal.as.sap.calendarcustom.view.DetailPage" /> -->
                </content>
            </Page>
                </f:midColumnPages>
               
            </f:FlexibleColumnLayout>
           
    
    
</mvc:View>